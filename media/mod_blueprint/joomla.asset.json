{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "mod_blueprint",
  "version": "1.0.0",
  "description": "BluePrint Assets for Joomla!",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "mod_blueprint.js.main",
      "type": "script",
      "uri": "mod_blueprint/main.js",
      "dependencies": [
        "core"
      ],
      "attributes": {
        "defer": true
      }
    },
    {
      "name": "mod_blueprint.js.es6",
      "type": "script",
      "uri": "mod_blueprint/es6_module.js",
      "dependencies": [
        "core", "mod_blueprint.js.main"
      ],
      "attributes": {
        "type": "module"
      }
    },
    {
      "name": "mod_blueprint.css.main",
      "type": "style",
      "uri": "mod_blueprint/main.css"
    }
  ]
}